[
  {
    "group_type": "flood_pipe",
    "description": "洪水-管道联合模拟",
    "parser_config": {
      "parser_module": "persistence.parsers.flood_pipe",
      "data_parsers": {
        "ne": "get_ne",
        "ns": "get_ns",
        "rainfall": "get_rainfall",
        "tide": "get_tide",
        "gate": "get_gate"
      },
      "action_appliers": {
        "gate_control": "apply_gate_action",
        "rainfall_modification": "apply_rainfall_action",
        "tide_adjustment": "apply_tide_action"
      },
      "model_data_mapping": {
        "ne_data": "ne",
        "ns_data": "ns",
        "rainfall_data": "rainfall",
        "tide_data": "tide",
        "gate_data": "gate"
      }
    },
    "shared": [
      {
        "name": "1d_data",
        "type": "dict"
      },
      {
        "name": "2d_data",
        "type": "dict"
      },
      {
        "name": "1d_ready",
        "type": "Event"
      },
      {
        "name": "2d_ready",
        "type": "Event"
      },
      {
        "name": "lock",
        "type": "Lock"
      }
    ],
    "processes": [
      {
        "name": "flood",
        "script": "coupled_0710.Flood_new",
        "entrypoint": "run_flood",
        "parameters": [
          {
            "name": "shared",
            "type": "dict"
          },
          {
            "name": "model_data",
            "type": "dict"
          },
          {
            "name": "inp_path",
            "type": "str"
          },
          {
            "name": "resource_path",
            "type": "str"
          },
          {
            "name": "step",
            "type": "int"
          },
          {
            "name": "flag",
            "type": "int"
          }
        ]
      },
      {
        "name": "pipe",
        "script": "coupled_0710.pipe_NH",
        "entrypoint": "run_pipe_simulation",
        "parameters": [
          {
            "name": "shared",
            "type": "dict"
          },
          {
            "name": "inp_path",
            "type": "str"
          },
          {
            "name": "resource_path",
            "type": "str"
          },
          {
            "name": "step",
            "type": "int"
          }
        ]
      }
    ],
    "monitor_config": {
      "file_types": [
        "result",
        "flood_nodes",
        "hsf"
      ],
      "file_suffix": {
        "result": ".dat",
        "flood_nodes": ".txt",
        "hsf": ".hsf"
      }
    }
  },
  {
    "group_type": "test",
    "description": "测试模拟",
    "shared": [
      {
        "name": "shared_data",
        "type": "dict"
      }
    ],
    "processes": [
      {
        "name": "test",
        "script": "model/test.py",
        "entrypoint": "run_pipe_simulation",
        "parameters": [
          {
            "name": "shared",
            "type": "dict"
          },
          {
            "name": "inp_path",
            "type": "str"
          },
          {
            "name": "resource_path",
            "type": "str"
          },
          {
            "name": "step",
            "type": "int"
          }
        ]
      }
    ],
    "monitor_config": {
      "file_types": [
        "result"
      ],
      "file_suffix": {
        "result": ".dat"
      }
    }
  }
]